<script type="text/javascript">
  function formatCurrency(a) {
      var b = parseFloat(a).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1.").toString();
      var len = b.length;
      b = b.substring(0, len - 3);
      return b;
  }


